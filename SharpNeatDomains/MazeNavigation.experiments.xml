      </NoveltyConfig>
      <MaxTimesteps>400</MaxTimesteps>
      <MinSuccessDistance>5</MinSuccessDistance>
      <MaxDistanceToTarget>300</MaxDistanceToTarget>
      <MazeVariant>OpenEndedMediumMaze</MazeVariant>
